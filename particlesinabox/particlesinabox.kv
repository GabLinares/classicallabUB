<GameScreen>:
	name: 'game'

	plotbox: plot_box
	enplotbox: enplot_box
	playbutton: play_button
	g_to_m: g_to_m_id

	BoxLayout:
		BoxLayout:
			id: plot_box
			size_hint: (None, 1)
			size: (self.height, self.height)
			on_touch_down: root.update_pos(args[1])
		BoxLayout:
			orientation: 'vertical'
			size_hint: (0.25, 1)
			BoxLayout:
				size_hint: (1, 0.1)
				orientation: 'horizontal'
				Button:
					id: g_to_m_id
					background_normal: 'icons/back_to_menu.png'
					background_down: 'icons/back_to_menu.png'
					on_press: root.transition_GM()
				Button:
					id: play_button
					#text: 'Play'
					background_normal: 'icons/play.png'
					background_down: 'icons/playb.png'
					on_press: root.play_button()
				Button:
					#text: 'Pause'
					background_normal: 'icons/pause.png'
					background_down: 'icons/pauseb.png'
					on_press: root.pause()
			BoxLayout:
				size_hint: (1, 0.5)
				orientation: 'horizontal'
				canvas.before:
					Color:
						rgb: 0/255.0, 0/255.0, 0/255.0
					Rectangle:
						pos: self.pos
						size: self.size
				TabbedPanel:
					id: plot_menu
					do_default_tab: False
					tab_width: 125
					background_image: 'Icons/black.png'
					TabbedPanelItem:
						text: 'Energy'
						background_down: 'Icons/blue.png'
						BoxLayout:
							id: enplot_box

<savewindow>:
	text_input: text_input
	BoxLayout:
		orientation: 'vertical'
		FileChooserListView:
			id: file_chooser
			on_selection: text_input.text = self.selection and self.selection[0] or ''
			rootpath: 'Saves'
		TextInput:
			id: text_input
			size_hint: (1, None)
			height: 30
			multiline: False
		BoxLayout:
			orientation: 'horizontal'
			size_hint: (1, None)
			height: 60
			Button:
				text: 'Save'
				on_press: root.save(file_chooser.path, text_input.text)
			Button:
				text: 'Cancel'
				on_press: root.cancel()

<loadwindow>:
	BoxLayout:
		orientation: 'vertical'
		FileChooserListView:
			id: file_chooser
			rootpath: 'Saves'
		BoxLayout:
			orientation: 'horizontal'
			size_hint: (1, None)
			height: 60
			Button:
				text: 'Load'
				on_press: root.load(file_chooser.path, file_chooser.selection)
			Button:
				text: 'Cancel'
				on_press: root.cancel()

<settingswindow>:
	rbig_slider: rbigslider
	boxlength_slider: boxlengthslider
	dt_slider: dtslider
	temp1_slider: temp1slider
	temp2_slider: temp2slider
	BoxLayout:
		pos_hint: {'right': 0.9, 'top': 1}
		size_hint: (0.9,0.8)
		orientation: 'vertical'
		BoxLayout:
			orientation: 'horizontal'
			Label:
				text: 'R[sub]big[/sub]: '
				markup: True
				size_hint: (0.15, 1)
			Label:
				text: str(round(rbigslider.value,2))
				size_hint: (0.05, 1)
			Slider:
				id: rbigslider
				size_hint: (0.5, 1)
				min: 1
				max: 5
				step: 1
				on_touch_up: root.change_settings()
		BoxLayout:
			orientation: 'horizontal'
			Label:
				text: 'Simulation length: '
				markup: True
				size_hint: (0.15, 1)
			Label:
				text: str(round(boxlengthslider.value,2))
				size_hint: (0.05, 1)
			Slider:
				id: boxlengthslider
				size_hint: (0.45, 1)
				min: 200
				max: 400
				step: 10
				on_touch_up: root.change_settings()
			Label:
				text: 'Angstroms (10[sup]-10[/sup]m)'
				markup: True
				size_hint: (0.15, 1)
		BoxLayout:
			orientation: 'horizontal'
			Label:
				text: 'Temperature[sub]1[/sub]: '
				markup: True
				size_hint: (0.15, 1)
			Label:
				text: str(round(temp1slider.value,2))
				size_hint: (0.05, 1)
			Slider:
				id: temp1slider
				size_hint: (0.5, 1)
				min: 1
				max: 5
				step: 1
				on_touch_up: root.change_settings()
		BoxLayout:
			orientation: 'horizontal'
			Label:
				text: 'Temperature[sub]2[/sub]: '
				markup: True
				size_hint: (0.15, 1)
			Label:
				text: str(round(temp2slider.value,2))
				size_hint: (0.05, 1)
			Slider:
				id: temp2slider
				size_hint: (0.5, 1)
				min: 1
				max: 5
				step: 1
				on_touch_up: root.change_settings()
		BoxLayout:
			orientation: 'horizontal'
			Label:
				text: 'Time step (dt): '
				markup: True
				size_hint: (0.15, 1)
			Label:
				text: str(round(dtslider.value,3))
				size_hint: (0.15, 1)
			Slider:
				id: dtslider
				size_hint: (0.5, 1)
				min: 0.01
				max: 0.02
				step: 0.005
				on_touch_up: root.change_settings()
	BoxLayout:
		pos_hint: {'right': 1}
		orientation: 'horizontal'
		size_hint: (1, None)
		height: 60
		Button:
			text: 'Exit'
			on_press: root.cancel()